 {% block body %}



     <div class="row">
         <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
             <div class="page-header">
                 <h2 class="pageheader-title">Liste des participations</h2>

                 <div class="page-breadcrumb">
                     <nav aria-label="breadcrumb">
                         <ol class="breadcrumb">
                             <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Liste des participations</a></li>

                         </ol>
                     </nav>
                 </div>
             </div>
         </div>
     </div>




     <div class="row">
         <!-- ============================================================== -->
         <!-- data table  -->
         <!-- ============================================================== -->
         <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
             <div class="card">
{#                 <a href="{{ path('ajouter_casting') }}"><button class="btn btn-outline-light buttons-copy buttons-html5" tabindex="3" aria-controls="example" type="button"><span>Ajouter Casting</span></button></a>#}

                 <div class="card-body">
                     <div class="table-responsive">
                         <table id="example" class="table table-striped table-bordered second" style="width:100%">
                             <thead>
                             <tr>
                                 <th>   Casting</th>
                                 <th>  User </th>
                                 <th> Resultat </th>


                                 <th> Actions  </th>


                             </tr>
                             </thead>
                             <tbody>
                             {% for p in data %}

                             <tr>
                                 <td> {{  p.nom}}  </td>
                                 <td> {{  p.name}}  </td>

                                 <td> {{  p.resultat}}   </td>

                                 <td>
                                     {% if p.resultat =='en cours' %}
                                     <div class="btn-group btn-group-sm btn-group-horizontal">
                                         <a class="btn btn-secondary"
                                            href="{{ path('gerer_Participation', { 'id': p.pid ,'token':'Admis'}) }}"
                                            data-toggle="tooltip" title="suprimer casting">
                                             <i class="f"> Accepter</i>
                                         </a>
                                     </div>
                                     <div class="btn-group btn-group-sm btn-group-horizontal">
                                         <a class="btn btn-secondary"
                                            href="{{ path('gerer_Participation', { 'id': p.pid ,'token':'Refuse'}) }}"
                                            data-toggle="tooltip" title="modifier casting">
                                             <i class=""> Refuser</i>
                                         </a>
                                     </div>
                                     {% else %}
                                     Participation Termin√©
                                     {% endif %}
                                 </td>






                                 {% endfor %}

                             </tfoot>
                         </table>
                     </div>
                 </div>
             </div>
         </div>
         <!-- ============================================================== -->
         <!-- end data table  -->
         <!-- ============================================================== -->
     </div>

 {% endblock %}

